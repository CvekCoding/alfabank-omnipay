{
  "name": "cvek/alfabank-omnipay",
  "description": "Omnipay driver for Alfabank",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "AndrewNovikof",
      "email": "andrewnovikofff@gmail.com"
    },
    {
      "name": "Logachev Sergey",
      "email": "serg@baitek.org"
    }
  ],
  "keywords": [
    "alfabank",
    "acquiring",
    "rest",
    "api",
    "payment"
  ],
  "require": {
    "php": "^7.1",
    "omnipay/common": "v3.0.2"
  },
  "require-dev": {
    "omnipay/tests": "^3",
    "phpunit/phpunit": "^5.7|^6",
    "squizlabs/php_codesniffer": "3.*"
  },
  "autoload": {
    "psr-4": {
      "Omnipay\\Alfabank\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Omnipay\\Alfabank\\Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
